type Person {
    id: Int!
    name: String!
    address: [Address]
    phone: String
}

type Address {
    type: AddressType!
    street: String
}

enum AddressType {
    PRIMARY,
    SECONDARY
}

type Query {
    person(id: Int!): Person
}

# Mutation

input PersonInput {
    id: Int!
    name: String!
}

input AddressInput {
    id: Int!

}